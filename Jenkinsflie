pipeline {
    agent any

    stages {
        stage('build') {
      steps {
        sh 'pip install -r requirements.txt'
      }
    }
        stage('Test') {
            steps {
                sh 'make check || true'
                sh 'pytest ./testSQL.py'
                sh 'pytest testSQL.py --html=report.html'
            }
        }
    }
}
